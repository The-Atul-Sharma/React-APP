{
  "ci": {
      "collect": {
          "url": [
              "http://localhost:3000"
          ],
          "startServerCommand": "npm run serve",
          "startServerReadyPattern": "Accepting connections at http://localhost:3000",
          "settings": {
              "onlyCategories": [
                  "accessibility",
                  "best-practices",
                  "performance",
                  "pwa",
                  "seo"

              ]
          }
      },
      "upload": {
          "target": "temporary-public-storage"
      }
  }
}