{
  "ci": {
      "collect": {
          "url": [
              "http://localhost:3000"
          ],
          "startServerCommand": "npm run start",
          "startServerReadyPattern": "Accepting connections at http://localhost:3000",
          "settings": {
              "onlyCategories": [
                  "accessibility"
              ]
          }
      },
      "upload": {
          "target": "temporary-public-storage"
      }
  }
}